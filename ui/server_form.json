{
    "long_form": {
        "controls": [
            {
                "custom_long_form@long-form.main": {
                    "bindings": [
                        {
                            "binding_name": "#title_text"
                        },
                        {
                            "binding_type": "view",
                            "source_property_name": "(not ((#title_text - '@mineville/long_form') = #title_text))",
                            "target_property_name": "#visible"
                        }
                    ]
                }
            },
            {
                "city_menu@city-menu.main": {
                    "bindings": [
                        {
                            "binding_name": "#title_text"
                        },
                        {
                            "binding_type": "view",
                            "source_property_name": "(not ((#title_text - '@mineville/city_menu') = #title_text))",
                            "target_property_name": "#visible"
                        }
                    ]
                }
            },
            {
                "crate_preview@crate-preview.main": {
                    "bindings": [
                        {
                            "binding_name": "#title_text"
                        },
                        {
                            "binding_type": "view",
                            "source_property_name": "(not ((#title_text - '@mineville/crate_preview') = #title_text))",
                            "target_property_name": "#visible"
                        }
                    ]
                }
            },
            {
                "gamemodes_menu@gamemodes-menu.main": {
                    "bindings": [
                        {
                            "binding_name": "#title_text"
                        },
                        {
                            "binding_type": "view",
                            "source_property_name": "(not ((#title_text - '@mineville/gamemodes_menu') = #title_text))",
                            "target_property_name": "#visible"
                        }
                    ]
                }
            },
            {
                "random_teleport_menu@earth-smp-rtp.main": {
                    "bindings": [
                        {
                            "binding_name": "#title_text"
                        },
                        {
                            "binding_type": "view",
                            "source_property_name": "(not ((#title_text - '@mineville/earth-smp/rtp_menu') = #title_text))",
                            "target_property_name": "#visible"
                        }
                    ]
                }
            },
            {
                "vanilla_long_form@common_dialogs.main_panel_no_buttons": {
                    "$title_panel": "common_dialogs.standard_title_label",
                    "$title_size": [
                        "100% - 15px",
                        10
                    ],
                    "$title_max_size": [
                        "100% - 15px",
                        10
                    ],
                    "size": [
                        225,
                        200
                    ],
                    "$text_name": "#title_text",
                    "$title_text_binding_type": "none",
                    "$child_control": "server_form.long_form_panel",
                    "layer": 2,
                    "bindings": [
                        {
                            "binding_name": "#title_text"
                        },
                        {
                            "binding_type": "view",
                            "source_property_name": "((#title_text - '@mineville') = #title_text)",
                            "target_property_name": "#visible"
                        }
                    ]
                }
            }            
        ]
    }
}