{
    "long_form": {
        "controls": [
            {
                "long_form@long-form.main": {
                    "bindings": [
                        {
                            "binding_name": "#title_text"
                        },
                        {
                            "binding_type": "view",
                            "source_property_name": "(not ((#title_text - '@mineville/long_form') = #title_text))",
                            "target_property_name": "#visible"
                        }
                    ]
                }
            },
            {
                "city_menu@city-menu.main": {
                    "bindings": [
