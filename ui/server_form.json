                            "binding_name": "#title_text"
                        },
                        {
                            "binding_type": "view",
                            "source_property_name": "(not ((#title_text - '@mineville/city_menu') = #title_text))",
                            "target_property_name": "#visible"
                        }
                    ]
                }
            },
            {
                "gamemodes_menu@gamemodes-menu.main": {
                    "bindings": [
                        {
                            "binding_name": "#title_text"
                        },
                        {
                            "binding_type": "view",
                            "source_property_name": "(not ((#title_text - '@mineville/gamemodes_menu') = #title_text))",
                            "target_property_name": "#visible"
                        }
                    ]
                }
            }
        ]
    }
