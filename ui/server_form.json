      "type": "panel",
      "size": [
         "100%",
         "100%"
      ],
      "controls": [
         {
            "server_form_factory": {
               "type": "factory",
               "control_ids": {
                  "long_form": "@server_form.switching_long_form",
                  "custom_form": "@server_form.custom_form"
               }
            }
         }
      ]
   },
