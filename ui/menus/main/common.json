{
    "namespace": "city-menu-common",
    "menu_template@common-mineville.form_template": {
        "$content_control": "",
        "$content_controls": [
            {
                "content_panel": {
                    "type": "panel",
                    "controls": [
                        {
                            "scroll_panel@common.scrolling_panel": {
                                "$show_background": false,
                                "$scrolling_content": "city-menu-common.scroll_content",
                                "$scroll_size": [
                                    "0px",
                                    "0px"
                                ],
                                "$scrolling_pane_offset": [
                                    "0px",
                                    "0px"
                                ],
                                "$scroll_bar_left_padding_size": [
                                    "0px",
                                    "0px"
                                ],
                                "$scroll_bar_right_padding_size": [
                                    "0px",
                                    "0px"
                                ]
                            }
                        }
                    ]
                }
            }
        ]
    },
    "scroll_content": {
        "type": "stack_panel",
        "orientation": "vertical",
        "collection_name": "form_buttons",
        "size": [
            "100%",
            "100%c"
        ],
        "controls": [
            {
                "navigation_tab@city-menu-common.navigation_tabs": {}
            },
            {
                "stack_panel_content@$content_control": {}
            }
        ]
    },
    "navigation_tabs": {
        "type": "stack_panel",
        "orientation": "horizontal",
        "collection_name": "form_buttons",
        "size": [
            "100% + 0.1px",
            "30px"
        ],
        "controls": [
            {
                "city_tab@city-menu-common.navigation_button": {
                    "collection_index": 49
                }
            },
            {
                "teleport_tab@city-menu-common.navigation_button": {
                    "collection_index": 48
                }
            },
            {
                "economy_tab@city-menu-common.navigation_button": {
                    "collection_index": 47
                }
            },
            {
                "cosmetics_tab@city-menu-common.navigation_button": {
                    "collection_index": 46
                }
            },
            {
                "quick_actions_tab@city-menu-common.navigation_button": {
                    "collection_index": 45
                }
            }
        ]
    },
    "navigation_button@common-mineville.form_button": {
        "size": [
            "20%",
            "100%"
        ],
        "bindings": [
            {
                "binding_name": "#null",
                "binding_type": "collection",
                "binding_condition": "none",
                "binding_collection_name": "form_buttons"
            },
            {
                "binding_name": "#null",
                "binding_type": "collection_details",
                "binding_collection_name": "form_buttons"
            },
            {
                "binding_name": "#form_button_text",
                "binding_type": "collection",
                "binding_collection_name": "form_buttons"
            }
        ],
        "$button_contents": [
            {
                "contents@city-menu-common.navigation_button_contents": {}
            }
        ]
    },
    "navigation_button_contents": {
        "type": "label",
        "text": "#text",
        "font_size": "small",
        "font_scale_factor": 1.5,
        "shadow": true,
        "text_alignment": "center",
        "layer": 1,
        "offset": [
            "0px",
            "-1px"
        ],
        "max_size": [
            "100%",
            "100%"
        ],
        "bindings": [
            {
                "binding_name": "#form_button_text",
                "binding_type": "collection",
                "binding_collection_name": "form_buttons"
            },
            {
                "binding_type": "view",
                "source_property_name": "(#form_button_text - '#locked')",
                "target_property_name": "#text"
            }
        ]
    },
    "text_image_field": {
        "type": "panel",
        "size": [
            "100%",
            "80px"
        ],
        "controls": [
            {
                "background": {
                    "type": "image",
                    "texture": "textures/ui/common/light_field"
                }
            },
            {
                "stack_panel": {
                    "type": "stack_panel",
                    "orientation": "horizontal",
                    "anchor_from": "left_middle",
                    "anchor_to": "left_middle",
                    "bindings": [
                        {
                            "binding_type": "collection_details",
                            "binding_collection_name": "form_buttons"
                        }
                    ],
                    "controls": [
                        {
                            "image_panel": {
                                "type": "panel",
                                "size": [
                                    "80px",
                                    "80px"
                                ],
                                "controls": [
                                    {
                                        "border": {
                                            "type": "image",
                                            "texture": "textures/ui/Black",
                                            "size": [
                                                "100% - 4px",
                                                "100% - 4px"
                                            ]
                                        }
                                    },
                                    {
                                        "image_panel": {
                                            "type": "panel",
                                            "size": [
                                                "100% - 6px",
                                                "100% - 6px"
                                            ],
                                            "bindings": [
                                                {
                                                    "binding_type": "collection",
                                                    "binding_collection_name": "form_buttons",
                                                    "binding_name": "#form_button_texture"
                                                },
                                                {
                                                    "binding_type": "view",
                                                    "source_property_name": "(not (#form_button_texture = ''))",
                                                    "target_property_name": "#visible"
                                                },
                                                {
                                                    "binding_type": "view",
                                                    "source_property_name": "(not (#form_button_texture = 'loading'))",
                                                    "target_property_name": "#visible"
                                                }
                                            ],
                                            "controls": [
                                                {
                                                    "image": {
                                                        "type": "image",
                                                        "texture": "#texture",
                                                        "size": [
                                                            "100% - 6px",
                                                            "100% - 6px"
                                                        ],
                                                        "bindings": [
                                                            {
                                                                "binding_type": "collection",
                                                                "binding_collection_name": "form_buttons",
                                                                "binding_name": "#form_button_texture",
                                                                "binding_name_override": "#texture"
                                                            },
                                                            {
                                                                "binding_type": "collection",
                                                                "binding_collection_name": "form_buttons",
                                                                "binding_name": "#form_button_texture_file_system",
                                                                "binding_name_override": "#texture_file_system"
                                                            }
                                                        ]
                                                    }
                                                }
                                            ]
                                        }
                                    }
                                ]
                            }
                        },
                        {
                            "padding": {
                                "type": "panel",
                                "size": [
                                    "10px",
                                    "0px"
                                ]
                            }
                        },
                        {
                            "label_panel": {
                                "type": "panel",
                                "layer": 1,
                                "size": [
                                    "100% - 80px",
                                    "100%"
                                ],
                                "controls": [
                                    {
                                        "label": {
                                            "type": "label",
                                            "text": "#text",
                                            "font_size": "small",
                                            "font_scale_factor": 1.5,
                                            "shadow": true,
                                            "max_size": [
                                                "330px - 80px - 20px",
                                                "80px"
                                            ],
                                            "bindings": [
                                                {
                                                    "binding_type": "collection",
                                                    "binding_collection_name": "form_buttons",
                                                    "binding_name": "#form_button_text",
                                                    "binding_name_override": "#text"
                                                }
                                            ]
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            }
        ]
    },
    "field_button": {
        "type": "panel",
        "$text_color": "white",
        "$button_color": "common",
        "controls": [
            {
                "panel": {
                    "type": "panel",
                    "bindings": [
                        {
                            "binding_type": "collection_details",
                            "binding_collection_name": "form_buttons"
                        },
                        {
                            "binding_type": "collection",
                            "binding_collection_name": "form_buttons",
                            "binding_name": "#form_button_texture"
                        }
                    ],
                    "controls": [
                        {
                            "button": {
                                "type": "button",
                                "default_control": "default",
                                "hover_control": "hover",
                                "pressed_control": "pressed",
                                "locked_control": "locked",
                                "sound_name": "random.click",
                                "focus_enabled": true,
                                "bindings": [
                                    {
                                        "binding_name": "#null",
                                        "binding_type": "collection",
                                        "binding_condition": "none",
                                        "binding_collection_name": "form_buttons"
                                    },
                                    {
                                        "binding_name": "#null",
                                        "binding_type": "collection_details",
                                        "binding_collection_name": "form_buttons"
                                    }
                                ],
                                "button_mappings": [
                                    {
                                        "from_button_id": "button.menu_select",
                                        "to_button_id": "button.form_button_click",
                                        "mapping_type": "pressed"
                                    },
                                    {
                                        "from_button_id": "button.menu_ok",
                                        "to_button_id": "button.form_button_click",
                                        "mapping_type": "focused"
                                    }
                                ],
                                "controls": [
                                    {
                                        "default": {
                                            "type": "image",
                                            "texture": "#texture",
                                            "bindings": [
                                                {
                                                    "binding_type": "collection",
                                                    "binding_collection_name": "form_buttons",
                                                    "binding_name": "#form_button_texture"
                                                },
                                                {
                                                    "binding_type": "view",
                                                    "source_property_name": "('textures/ui/common/buttons/' + $button_color + '/default' - #form_button_texture)",
                                                    "target_property_name": "#texture"
                                                }
                                            ],
                                            "controls": [
                                                {
                                                    "label": {
                                                        "type": "label",
                                                        "text": "#text",
                                                        "color": "$text_color",
                                                        "layer": 1,
                                                        "bindings": [
                                                            {
                                                                "binding_type": "collection",
                                                                "binding_collection_name": "form_buttons",
                                                                "binding_name": "#form_button_text",
                                                                "binding_name_override": "#text"
                                                            }
                                                        ]
                                                    }
                                                }
                                            ]
                                        }
                                    },
                                    {
                                        "hover": {
                                            "type": "image",
                                            "texture": "#texture",
                                            "bindings": [
                                                {
                                                    "binding_type": "collection",
                                                    "binding_collection_name": "form_buttons",
                                                    "binding_name": "#form_button_texture"
                                                },
                                                {
                                                    "binding_type": "view",
                                                    "source_property_name": "('textures/ui/common/buttons/' + $button_color + '/hover' - #form_button_texture)",
                                                    "target_property_name": "#texture"
                                                }
                                            ],
                                            "controls": [
                                                {
                                                    "label": {
                                                        "type": "label",
                                                        "text": "#text",
                                                        "color": "$text_color",
                                                        "layer": 1,
                                                        "bindings": [
                                                            {
                                                                "binding_type": "collection",
                                                                "binding_collection_name": "form_buttons",
                                                                "binding_name": "#form_button_text",
                                                                "binding_name_override": "#text"
                                                            }
                                                        ]
                                                    }
                                                }
                                            ]
                                        }
                                    },
                                    {
                                        "pressed": {
                                            "type": "image",
                                            "texture": "#texture",
                                            "bindings": [
                                                {
                                                    "binding_type": "collection",
                                                    "binding_collection_name": "form_buttons",
                                                    "binding_name": "#form_button_texture"
                                                },
                                                {
                                                    "binding_type": "view",
                                                    "source_property_name": "('textures/ui/common/buttons/' + $button_color + '/pressed' - #form_button_texture)",
                                                    "target_property_name": "#texture"
                                                }
                                            ],
                                            "controls": [
                                                {
                                                    "label": {
                                                        "type": "label",
                                                        "text": "#text",
                                                        "color": "$text_color",
                                                        "layer": 1,
                                                        "offset": [
                                                            "0px",
                                                            "1px"
                                                        ],
                                                        "bindings": [
                                                            {
                                                                "binding_type": "collection",
                                                                "binding_collection_name": "form_buttons",
                                                                "binding_name": "#form_button_text",
                                                                "binding_name_override": "#text"
                                                            }
                                                        ]
                                                    }
                                                },
                                                {
                                                    "overlay": {
                                                        "type": "image",
                                                        "texture": "textures/ui/Black",
                                                        "alpha": 0.3,
                                                        "layer": 1
                                                    }
                                                }
                                            ]
                                        }
                                    },
                                    {
                                        "locked": {
                                            "type": "image",
                                            "texture": "#texture",
                                            "bindings": [
                                                {
                                                    "binding_type": "collection",
                                                    "binding_collection_name": "form_buttons",
                                                    "binding_name": "#form_button_texture"
                                                },
                                                {
                                                    "binding_type": "view",
                                                    "source_property_name": "('textures/ui/common/buttons/' + $button_color + '/pressed' - #form_button_texture)",
                                                    "target_property_name": "#texture"
                                                }
                                            ],
                                            "controls": [
                                                {
                                                    "label": {
                                                        "type": "label",
                                                        "text": "#text",
                                                        "color": "$text_color",
                                                        "layer": 1,
                                                        "offset": [
                                                            "0px",
                                                            "1px"
                                                        ],
                                                        "bindings": [
                                                            {
                                                                "binding_type": "collection",
                                                                "binding_collection_name": "form_buttons",
                                                                "binding_name": "#form_button_text",
                                                                "binding_name_override": "#text"
                                                            }
                                                        ]
                                                    }
                                                },
                                                {
                                                    "overlay": {
                                                        "type": "image",
                                                        "texture": "textures/ui/Black",
                                                        "alpha": 0.3,
                                                        "layer": 1
                                                    }
                                                }
                                            ]
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            }
        ]
    },
    "description_field": {
        "type": "panel",
        "size": [
            "100%",
            "15px"
        ],
        "controls": [
            {
                "background": {
                    "type": "image",
                    "texture": "textures/ui/common/buttons/white/pressed",
                    "alpha": 0.65
                }
            },
            {
                "panel": {
                    "type": "panel",
                    "bindings": [
                        {
                            "binding_type": "collection_details",
                            "binding_collection_name": "form_buttons"
                        }
                    ],
                    "controls": [
                        {
                            "label": {
                                "type": "label",
                                "font_type": "MinecraftTen",
                                "font_scale_factor": 1.0,
                                "layer": 2,
                                "text": "#text",
                                "color": "black",
                                "offset": [
                                    "0px",
                                    "-1px"
                                ],
                                "max_size": [
                                    "100%",
                                    "100%"
                                ],
                                "bindings": [
                                    {
                                        "binding_type": "collection",
                                        "binding_collection_name": "form_buttons",
                                        "binding_name": "#form_button_text",
                                        "binding_name_override": "#text"
                                    }
                                ]
                            }
                        }
                    ]
                }
            }
        ]
    },
    "button_grid": {
        "type": "grid",
        "grid_item_template": "city-menu-common.grid_button",
        "collection_name": "form_buttons",
        "grid_fill_direction": "vertical",
        "grid_rescaling_type": "horizontal",
        "anchor_from": "top_middle",
        "anchor_to": "top_middle",
        "size": [
            "100%",
            "100%c"
        ],
        "bindings": [
            {
                "binding_type": "view",
                "binding_condition": "once",
                "source_property_name": "(#form_text * 1)",
                "target_property_name": "#maximum_grid_items"
            }
        ]
    },
    "grid_button@city-menu-common.field_button": {
        "$button_color": "white",
        "$text_color": [
            0.3,
            0.3,
            0.3
        ],
        "size": [
            "50%",
            "30px"
        ]
    }
}